{
  "name": "express",
  "version": "1.0.0",
  "description": "Generate Dev & Prod builds for Node/Express app using Webpack",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node ./index.js",
    "start:dev": "node ./dev-build/main.js",
    "start:prod": "node ./prod-build/main.js",
    "start:prod:optimization": "node ./prod-build-optimization/main.js",
    "build:dev": "webpack --config webpack.dev.js --mode development",
    "build:prod": "webpack --config webpack.prod.js --mode production"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@babel/preset-env": "^7.20.2",
    "compress": "^0.99.0",
    "compression": "^1.7.4",
    "express": "^4.18.2",
    "serverless-http": "^3.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "babel-loader": "^9.1.2",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-node-externals": "^3.0.0"
  }
}
